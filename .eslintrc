{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "mocha": true,
    "es6": true
  },
  "rules":{
    "no-param-reassign": "off",
    "no-underscore-dangle": "off",
    "max-len": "off"
  },
  "extends": "airbnb"
}
